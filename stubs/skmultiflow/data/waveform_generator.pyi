from skmultiflow.data.base_stream import Stream as Stream
from skmultiflow.utils import check_random_state as check_random_state
from typing import Any, Optional

class WaveformGenerator(Stream):
    random_state: Any = ...
    n_num_features: Any = ...
    n_classes: Any = ...
    n_targets: int = ...
    name: str = ...
    n_features: Any = ...
    feature_names: Any = ...
    target_names: Any = ...
    target_values: Any = ...
    def __init__(self, random_state: Optional[Any] = ..., has_noise: bool = ...) -> None: ...
    @property
    def has_noise(self): ...
    @has_noise.setter
    def has_noise(self, has_noise: Any) -> None: ...
    current_sample_x: Any = ...
    current_sample_y: Any = ...
    def next_sample(self, batch_size: int = ...): ...
